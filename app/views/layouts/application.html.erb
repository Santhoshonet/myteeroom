<head>
 <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
 <title>myteeroom - Aweesome tees for everyone</title>
 <%= stylesheet_link_tag "stylesheet" %>
 <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'/>
 <%= stylesheet_link_tag "jquery-ui-1.8.9.custom" %>
 	<!-- jQuery and Custom scripts -->
 	<%= javascript_include_tag "jquery-1.5.2.min.js" %>
 	<%	javascript_include_tag "jquery.cycle.lite.1.0.min.js" %>
 	<%= javascript_include_tag "jquery.cycle.lite.1.0.min.js" %>
	<%= javascript_include_tag "custom_scripts.js"%>
	<%= javascript_include_tag "jquery.roundabout.min.js" %>
 	<%= javascript_include_tag "jquery-ui-1.8.9.custom.min.js" %>
 	<%= javascript_include_tag "tipsy/jquery.tipsy.js" %>
 	<%= stylesheet_link_tag "css.tipsy" %>
	<%= javascript_include_tag "tabs.js"%>
 	<%= yield :head %>

