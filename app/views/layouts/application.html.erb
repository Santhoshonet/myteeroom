	</script>

</head>

<body>
<!-- MAIN WRAPPER -->
<div id="container"> 
  <!-- HEADER -->
  <div id="header">
    <div class="inner">
      <ul class="main_menu menu_left">
        <li><%= link_to 'All Tees', :controller => :store, :action => :list %></li>
        <li><a href="/">Home</a></li>
      </ul>
      <div id="logo"><a href="/"><%= image_tag "logo.png", :width => 217, :height => 141, :alt => "myteeroom store" %></a></div>
      <ul class="main_menu menu_right">
				<li><a href="about.html">About Us</a></li>
        <li><a href="contact.html">Contact Us</a></li>
      </ul>
      <div class="menu">
        <ul id="topnav">
            <li><%= link_to 'Guys', :controller => :store, :action => :list %></li>
            <li><%= link_to 'Girls', :controller => :store, :action => :list %></li>
            <li><%= link_to 'Kids', :controller => :store, :action => :list %></li>
        </ul>
      </div>
    </div>
  </div>
  <!-- END OF HEADER --> 
  
	<%= yield %>
  
  <!-- PRE-FOOTER -->
  <div id="pre_footer">
    <div class="inner">
      <div id="twitter_div"> <a class="twitter_follow" href="http://twitter.com/myteeroom" title="Follow Us on
      Twitter">Follow Us on Twitter</a> <a id="tweet_prev" href="#">Previous</a> <a id="tweet_next" href="#">Next</a>
        <ul id="twitter_update_list">
          <li>&nbsp;</li>
        </ul>
      </div>
    </div>
  </div>
  <!-- END OF PRE-FOOTER --> 
  
  <!-- FOOTER -->
  <div id="footer">
    <div class="inner">
      <div class="column_big"> 
        <!-- FOOTER MODULES AREA -->
        <div class="footer_modules">
          <h3>About myteeroom</h3>
          <p>We are a small group that got tired of have no t-shirt options in Indian and Malaysia,
					 so we did the simple thing and started our own.</p>
        </div>
        <!-- END OF FOOTER MODULES AREA -->
        <div class="footer_social">
          <h3>Spread the word</h3>
          <!-- AddThis Button BEGIN -->
          <div class="addthis_toolbox addthis_default_style "><script type="text/javascript">
      //<![CDATA[
            document.write('<a class="addthis_button_google_plusone" g:plusone:size="medium"></a> <a class="addthis_button_tweet"></a> <a class="addthis_button_facebook_like" fb:like:layout="button_count"></a>');
      //]]>
     </script> 
          </div>
          <script type="text/javascript" src="http://s7.addthis.com/js/250/addthis_widget.js#pubid=ra-4e7280075406aa87"></script> 
          <!-- AddThis Button END --> 
        </div>
      </div>
      <div class="column_small">
        <h3>Customer Service</h3>
        <ul>
          <li><a href="#">Home</a></li>
          <li><a href="#">Order History</a></li>
          <li><a href="#">Wishlist</a></li>
          <li><a href="#">Newsletter</a></li>
          <li><a href="#">Returns</a></li>
        </ul>
      </div>
      <div class="column_small">
        <h3>Information</h3>
        <ul>
          <li><a href="about.html">About Us</a></li>
          <li><a href="#">Delivery Information</a></li>
          <li><a href="#">Privacy policy</a></li>
          <li><a href="#">Terms and conditions</a></li>
          <li><a href="contact.html">Contact Us</a></li>
          <li><a href="sitemap.html">site map</a></li>
        </ul>
      </div>
      <div class="column_small">
        <h3>Extras</h3>
        <ul>
          <li><a href="#">Gift vouchers</a></li>
          <li><a href="#">Affiliates</a></li>
          <li><a href="#">Specials</a></li>
        </ul>
      </div>
      <div class="clear"></div>
      <span class="copyright">All copyright reserved <a href="http://myteeroom.com">myteeroom</a>. </span> </div>
  </div>
  <!-- END OF FOOTER --> 
  
</div>
<!-- END OF MAIN WRAPPER --> 
<script type="text/javascript" src="http://twitter.com/javascripts/blogger.js"></script> 
